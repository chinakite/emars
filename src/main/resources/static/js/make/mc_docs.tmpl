{% for doc in docs %}
<div class="file-list-item">
    <div class="file-list-title text-default">{{doc.name}}</div>
    <div class="file-list-description">{{doc.desc}}</div>
    <div class="file-list-time">{{doc.createTimeText}}</div>
    <a class="file-list-icon file-list-icon-1 fa fa-download bg-success" target="_blank" href="/make/downloadMcProductDoc?id={{doc.id}}"></a>
    {% if editable == true %}
    <div class="file-list-icon file-list-icon-2 fa fa-remove bg-danger" onclick="MAKECONTRACTPAGE.deleteDod('{{doc.id}}', '{{doc.name}}', '{{doc.type}}');"></div>
    {% endif %}
</div>
{% endfor %}
